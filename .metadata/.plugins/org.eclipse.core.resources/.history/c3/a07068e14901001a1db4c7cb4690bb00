package programe;

interface mohan {
public void run();
}
interface show{
	public void num();
	
}

class ram implements mohan,show{
	public void run() {
		System.out.println("hello word");
	}
	public void num() {
		System.out.println("green word");
	}
	public  static void main(String[]args) {
		ram a=new ram();
		a.run();
		a.num();
	}
}
