
 class ram{
	 void display() {
		 System.out.println("good evening");
	 }
 }
 class mohan extends ram{
	 void add(){
		 System.out.println("good day"); 
	 }
 }
 class sonu extends mohan{
	 void num(){
		 System.out.println("hello word"); 
	 }
 
	 public static void main(String[] args) {
	
	mohan s1=new mohan();
	sonu s2=new sonu();
	s1.display();
	s1.add();
	s2.num();		
	}
	 }

		
	
	



		


	
	
	
		
		
		
		
	

		
		
	




	


	
	

			
			
	
	 
	
	 
		
	
		

		  
		    
		  

		  
		    
		    
		  
		

