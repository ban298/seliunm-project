package program;

public class loop { 
	public void run()throws ArithmeticException{
		int a=100;
		int b=0;
		int  c=a/b;
		System.out.println(c);
		
	 }
	public static void main(String[]args) {
		loop a=new loop();
		try {
			a.run();	
		}
		catch(ArithmeticException e) {
			System.out.println("error handling");
		}
		System.out.println("complete parogram");
		
	}
	 
	 
   }


		  
		 
				 
			 



		 
		
	

		
		
		
		

		
		

	



