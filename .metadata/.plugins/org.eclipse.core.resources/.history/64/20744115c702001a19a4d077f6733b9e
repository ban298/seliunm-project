package program;
class loop{
	public static void run() {
		System.out.println("hello word");
	}
	public static void print() {
		System.out.println("hello green");
	}
	public static void main(String[]args) {
		
		loop.run();
		loop.print();
	}
	
			
		
		
	}

 
	
		

	
	



	 
	 
   


		  
		 
				 
			 



		 
		
	

		
	
		
		

		
		

	



