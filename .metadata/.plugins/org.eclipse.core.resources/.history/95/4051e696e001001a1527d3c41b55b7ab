package program;

public class loop { 
	public static void main(String[]args) {
		for(int i=1;i<10;i++) {
		System.out.println("hello word");
		System.out.println("hello green");
		System.out.println("hello red");
		System.out.println("green word");
	}
	
	
	}
}
	 
	 
   


		  
		 
				 
			 



		 
		
	

		
		
		
		

		
		

	



