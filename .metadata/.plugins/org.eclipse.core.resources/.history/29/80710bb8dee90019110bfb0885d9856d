	 void add(String b) {
		 System.out.println("arun pal"); 
	 }
		 public static void main(String [] args) {
		  ram s1=new ram();
		 s1.display("upendra");
		 mohan s2=new mohan();
		 s2.add("arun pal");
	
 
 
	
	 
	
	
	
			
