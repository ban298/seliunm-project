package program;
class loop{
	int x,y;
	loop(int a,int b){
		x=a;
		y=b;
		System.out.println("x +x");
		System.out.println("y +y");
		System.out.println("paramertrized counstouer");
	}
	public static void main(String[]args ) {
		loop a=new loop(10,20);
	}
			
		
		
	}

 
	
		

	
	



	 
	 
   


		  
		 
				 
			 



		 
		
	

		
		
		
		

		
		

	



